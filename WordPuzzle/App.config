<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.8"/>
    </startup>
  <appSettings>
    <add key="APP_TITLE" value="Word Puzzle"/>
    <add key="DESIGN_TIME_SCREEN_WIDTH" value="1920"/>
    <add key="DESIGN_TIME_SCREEN_HEIGHT" value="1080"/>
  
    <add key="GRID_SIZE" value="18"/> <!--Grid size is 1-based, e.g. for 18 grids it is 18-->
    <add key="MAX_WORDS" value="14"/>
    <add key="MAX_SCORES" value="14"/>
  </appSettings>
</configuration>
